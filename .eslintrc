{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "settings": {
    "react": {
      "version": "16.13"
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}
